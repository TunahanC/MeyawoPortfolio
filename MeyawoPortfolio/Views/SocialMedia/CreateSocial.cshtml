
@{
    ViewBag.Title = "CreateSocial";
    Layout = "~/Views/AdminLayout/_Layout.cshtml";
}

<h2>Yeni Medya Girişi</h2>

<form method="post">
    <label>Medya Başlığı</label>
    <input type="text" class="table table-bordered" name="Title" />
    <br />
    <label>Medya Url</label>
    <input type="text" class="table table-bordered" name="SocialMediaUrl" />
    <br />
    <label>İcon Url</label>
    <input type="text" class="table table-bordered" name="İcon" />
    <br />
    <button class="btn btn-info">Medya Kaydet</button>

</form>

